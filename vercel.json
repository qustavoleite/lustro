{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://lustro-black.vercel.app/"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
